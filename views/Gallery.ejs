<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title%></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p><%= message %></p>
    <ul>
        <% items.forEach((items)=>{ %>
           <li><%= items %></li>
      <%  }) %>
    </ul>
    <table border="1" cellpadding="10" cellspacing="10">
        <thead>
            <th>Name</th>
            <th>Age</th>
            <th>University</th>
        </thead>
        <tbody>
           <% data.forEach((data)=>{ %>
            <tr>
            <td><%= data.name _%></td>
            <td><%= data.age _%></td>
            <td><%= data.university _%></td>
            </tr>
         <%  }) %>
        </tbody>
    </table>
  </body>
</html>
